{
  "name": "nc-test-app",
  "version": "1.0.0",
  "description": "Web app for functional testing of NCSOFT projects",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "export NODE_ENV=development; nodemon index.js",
    "start-client": "export NODE_ENV=development; webpack-dev-server --env=dev --hot --progress --host 0.0.0.0 --config webpack.dev.config.js",
    "bs": "export NODE_ENV=production; export LIVE=false; webpack --env=prod --config webpack.server.config.js",
    "bc": "export NODE_ENV=production; export LIVE=false; webpack --env=prod --config webpack.prod.config.js",
    "build": "npm run clean && npm run bc && npm run bs",
    "open:dist": "export NODE_ENV=production; export LIVE=false; export PORT=3001; nodemon index.js",
    "build:open": "npm run build && npm run open:dist",
    "test": "babel-node test/unit-test.js",
    "test:int": "babel-node test/int-test.js",
    "test:update": "babel-node test/util/update.js"
  },
  "author": "NCSOFT",
  "license": "ISC",
  "engine": "node 8.9.4",
  "dependencies": {
    "aws-sdk": "^2.202.0",
    "axios": "^0.17.1",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "express": "^4.16.2",
    "mongoose": "^5.0.7",
    "nightmare": "^3.0.1",
    "nightmare-window-manager": "^0.2.2",
    "pug": "^2.0.0-rc.4",
    "socket.io": "^2.1.0",
    "xvfb": "^0.2.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chartist": "^0.11.0",
    "copy-webpack-plugin": "^4.4.2",
    "css-loader": "^0.28.10",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.9",
    "html-webpack-plugin": "^2.30.1",
    "material-ui": "^1.0.0-beta.34",
    "material-ui-icons": "^1.0.0-beta.17",
    "moment": "^2.20.1",
    "node-sass": "^4.7.2",
    "pug-loader": "^2.3.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-promise": "^0.5.3",
    "rimraf": "^2.6.2",
    "rsync": "^0.6.1",
    "sass-loader": "^6.0.6",
    "socket.io-client": "^2.1.0",
    "style-loader": "^0.20.2",
    "title-case": "^2.1.1",
    "webpack": "^3.11.0",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-dev-server": "^2.11.1",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-node-externals": "^1.6.0"
  }
}
